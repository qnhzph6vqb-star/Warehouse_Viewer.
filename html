function qs(name){return new URLSearchParams(location.search).get(name)}
function render(loc){
  document.getElementById('loc').textContent = loc||'';
  const box = document.getElementById('content');
  const rows = DATA[loc]||[];
  if(!loc){box.innerHTML='<div class="empty">No location selected. Add ?loc=K10-03 to the link.</div>';return;}
  if(rows.length===0){box.innerHTML='<div class="empty">No items in this location</div>';return;}
  let html='<table><thead><tr><th>Code</th><th>Name</th><th>Batch</th><th>Qty</th><th>Expiry</th></tr></thead><tbody>';
  for(var i=0;i<rows.length;i++){
    var r=rows[i];
    html+='<tr><td>'+ (r.code||'') +'</td><td>'+ (r.name||'') +'</td><td>'+ (r.batch||'') +'</td><td>'+ (r.qty||'') +'</td><td>'+ (r.exp||'') +'</td></tr>';
  }
  html+='</tbody></table>'; box.innerHTML=html;
}
function go(){
  var v=document.getElementById('locInput').value.trim();
  if(!v) return;
  var base=location.href.split('?')[0];
  location.href=base+'?loc='+encodeURIComponent(v);
}
render(qs('loc'));
</script>
</body></html>
